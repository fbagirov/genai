{
  "title": "LLM Inference Service",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Requests / s",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(inference_requests_total[1m]))"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 6
      }
    },
    {
      "type": "stat",
      "title": "Tokens / s",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(tokens_generated_total[1m]))"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 6
      }
    },
    {
      "type": "stat",
      "title": "p95 latency (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(inference_request_latency_seconds_bucket[5m])) by (le))"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 6
      }
    },
    {
      "type": "graph",
      "title": "Requests by status",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(inference_requests_total[1m])) by (status_code)"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "graph",
      "title": "In-flight requests",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "inference_inflight_requests"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 6,
        "w": 12,
        "h": 8
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}